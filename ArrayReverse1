function reverse(arr){
    let n =Math.floor (arr.length/2);
    for( let i = 0; i <= n-1; i++){
        let leftI = i;
        let rightI = arr.length-1-i;
        let lValue = arr[leftI];
        let rValue = arr[rightI];
        arr[leftI] = rValue;
        arr[rightI] = lValue;
    }
    return arr;
}

 let result1 = reverse([1,2,3,4,5]);
 let result2 = reverse([1,2,3,4,5,6]);
 let result3 = reverse([1,2,3,4,5,6,7]);
 console.log(result1)
 console.log(result2)
 console.log(result3)
